{
  "name": "ipstack",
  "version": "0.0.1",
  "description": "An app for storing user's geolocation",
  "author": "kamilzielinskidev",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "engines": {
    "node": "12.13.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "tsc": "tsc",
    "tslint:all": "tslint -c tslint.json 'src/**/*.ts'",
    "watch": "nodemon --watch src --ext ts --exec npm run start:dev"
  },
  "dependencies": {
    "@marblejs/core": "^3.4.5",
    "@marblejs/middleware-body": "^3.4.5",
    "@marblejs/middleware-logger": "^3.4.5",
    "@typegoose/typegoose": "^7.4.2",
    "chalk": "^4.1.0",
    "mongodb": "^3.6.3",
    "mongoose": "5.10.15",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.1",
    "fp-ts": "^2.8.6",
    "heroku": "^7.47.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  }
}
